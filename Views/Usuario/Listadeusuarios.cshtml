@model List<Usuario>
@{

    ViewData["Title"] = "Lista de Usuários";
}

<div class="lista-usuarios">
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nível</th>
                <th>Nome</th>
                <th>Data de nascimento</th>
                <th>Email</th>
                <th>Telefone</th>
                <th>Login</th>
                <th>Ações</th>
                <th>#</th>
            </tr>

        </thead>
        <tbody>
            @foreach (Usuario u in Model)
            {
                <tr>
                    <td>@u.IdUsuario</td>
                    <td>@u.Nivel</td>
                    <td>@u.Nome</td>
                    <td>@u.Nascimento</td>
                    <td>@u.Email</td>
                    <td>@u.Telefone</td>
                    <td>@u.Login</td>
                    <td><a href="/Usuario/Editar?IdUsuario=@u.IdUsuario">Editar</a></td>
                    <td><a href="/Usuario/Excluir?IdUsuario=@u.IdUsuario">Excluir</a></td>

                </tr>

            }

        </tbody>

    </table>
</div>
